{
    "name": "phel-lang",
    "displayName": "Phel Lang",
    "description": "Syntax highlighting for the Phel language",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.44.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "phel",
            "aliases": ["Phel", "phel"],
            "extensions": [".phel"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "phel",
            "scopeName": "source.phel",
            "path": "./syntaxes/phel.tmLanguage.json"
        }]
    }
}